{
  "name": "Yggdrasil on ChromeOS",
  "version": "4.0.0",
  "description": "Access the Yggdrasil network on ChromeOS.",
  "permissions": [
    "proxy"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "128": "y.128.png"
    }
  },
  "icons": {"128": "y-green.128.png"},
  "manifest_version": 2
}
